{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/v1/common/productSearch_object.json",
    "type": "object",
    "title": "ProductSearch object",
    "description": "",
    "default": {},
    "additionalProperties": false,
    "properties": {
        "$schema": {
            "type": "string",
            "format": "uri",
            "description": "$schema only included to aid validation of JSON, should not be used in real calls."
        },
        "AdFormatTypes": {
            "$id": "#/properties/AdFormatTypes",
            "type": "array",
            "title": "Ad Format Types",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/AdFormatTypes/items",
                "type": "string",
                "title": "The Items Schema"
            }
        },
        "Currency": {
            "$id": "#/properties/Currency",
            "type": "string",
            "title": "Currency"
        },
        "DeliveryType": {
            "$id": "#/properties/DeliveryType",
            "type": "string",
            "title": "Delivery Type"
        },
        "Domain": {
            "$id": "#/properties/Domain",
            "type": "string",
            "title": "Domain"
        },
        "Geometry": {
            "$id": "#/properties/AdvertiserId",
            "type": "array",
            "title": "Geometry",
            "description": "A list of ad format sizes that the product supports.",
            "items": {
                "$ref": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/v1/common/size_object.json"
            }
        },
        "Targeting": {
            "$id": "#/properties/TargetTypes",
            "$ref": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/v1/common/targetTypes_array.json"
        }
    }
}